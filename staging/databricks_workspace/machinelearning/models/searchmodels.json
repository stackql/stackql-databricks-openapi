      "application/json": {
       "schema": {
        "type": "object",
        "properties": {
         "registered_models": {
          "type": "array",
          "items": {
           "type": "object",
           "properties": {
            "name": {
             "type": "string"
            },
            "creation_timestamp": {
             "type": "integer"
            },
            "last_updated_timestamp": {
             "type": "integer"
            },
            "user_id": {
             "type": "string"
            },
            "description": {
