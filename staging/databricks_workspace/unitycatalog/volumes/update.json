{
 "/api/2.1/unity-catalog/volumes/{name}": {
  "patch": {
   "operationId": "volumes-update",
   "externalDocs": {
    "url": "https://docs.databricks.com/api/workspace/volumes/update"
   },
   "x-stackQL-resource": "volumes",
   "x-stackQL-method": "update",
   "x-stackQL-verb": "update",
   "x-numReqParams": 1,
   "parameters": [
    {
     "name": "name",
     "required": true,
     "description": "(string) The three-level (fully qualified) name of the volume",
     "in": "path"
    }
   ],
   "requestBody": {
    "required": true,
    "content": {
     "application/json": {
      "schema": {
       "type": "object",
       "properties": {
        "owner": {
         "type": "string",
         "description": "The identifier of the user who owns the volume"
        },
        "comment": {
         "type": "string",
         "description": "The comment attached to the volume"
        },
        "new_name": {
         "type": "string",
         "description": "New name for the volume."
        }
       },
       "example": {
        "new_name": "my_new_volume"
       }
      }
     }
    }
   },
   "responses": {
    "200": {
     "description": "Successfully updated the properties of the volume"
    }
   },
   "description": "Updates the specified volume under the specified parent catalog and schema."
  }
 }
}