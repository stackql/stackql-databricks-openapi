{
 "/api/2.1/unity-catalog/schemas/{full_name}": {
  "patch": {
   "operationId": "schemas-update",
   "externalDocs": {
    "url": "https://docs.databricks.com/api/workspace/schemas/update"
   },
   "x-stackQL-resource": "schemas",
   "x-stackQL-method": "update",
   "x-stackQL-verb": "update",
   "x-numReqParams": 1,
   "parameters": [
    {
     "name": "full_name",
     "required": true,
     "description": "(string) Full name of the schema.",
     "in": "path"
    }
   ],
   "requestBody": {
    "required": true,
    "content": {
     "application/json": {
      "schema": {
       "type": "object",
       "properties": {
        "owner": {
         "type": "string",
         "description": "Username of current owner of schema."
        },
        "comment": {
         "type": "string",
         "description": "User-provided free-form text description."
        },
        "enable_predictive_optimization": {
         "type": "string",
         "description": "Whether predictive optimization should be enabled for this object and objects under it."
        },
        "new_name": {
         "type": "string",
         "description": "New name for the schema."
        },
        "properties": {
         "type": "object"
        }
       },
       "example": {
        "owner": "string",
        "comment": "string",
        "properties": {
         "property1": "string",
         "property2": "string"
        },
        "enable_predictive_optimization": "DISABLE",
        "new_name": "string"
       }
      }
     }
    }
   },
   "responses": {
    "200": {
     "description": "The schema was successfully updated."
    }
   },
   "description": "Updates a schema for a catalog. The caller must be the owner of the schema or a metastore admin. If the caller is a metastore admin, only the"
  }
 }
}