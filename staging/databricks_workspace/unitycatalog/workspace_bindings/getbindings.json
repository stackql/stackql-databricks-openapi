{
 "/api/2.1/unity-catalog/bindings/{securable_type}/{securable_name}": {
  "get": {
   "operationId": "workspace-bindings-getbindings",
   "externalDocs": {
    "url": "https://docs.databricks.com/api/workspace/workspacebindings/getbindings"
   },
   "x-stackQL-resource": "workspace_bindings",
   "x-stackQL-method": "getbindings",
   "x-stackQL-verb": "select",
   "x-numReqParams": 2,
   "parameters": [
    {
     "name": "securable_type",
     "required": true,
     "description": "(string) The type of the securable to bind to a workspace.",
     "in": "path"
    },
    {
     "name": "securable_name",
     "required": true,
     "description": "(string) The name of the securable.",
     "in": "path"
    },
    {
     "name": "max_results",
     "description": "Maximum number of workspace bindings to return.",
     "in": "query"
    },
    {
     "name": "page_token",
     "description": "Opaque pagination token to go to next page based on previous query.",
     "in": "query"
    }
   ],
   "responses": {
    "200": {
     "description": "A list of workspace IDs that are bound to the securable",
     "content": {
      "application/json": {
       "schema": {
        "type": "object",
        "properties": {
         "bindings": {
          "type": "array",
          "items": {
           "type": "object",
           "properties": {
            "workspace_id": {
             "type": "integer"
            },
            "binding_type": {
             "type": "string"
            }
           }
          }
         },
         "next_page_token": {
          "type": "string"
         }
        }
       }
      }
     }
    }
   },
   "x-stackQL-objectKey": "$.bindings",
   "description": "Gets workspace bindings of the securable. The caller must be a metastore admin or an owner of the securable."
  }
 }
}