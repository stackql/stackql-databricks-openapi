{
  "verb": "get",
  "path": "/api/2.0/accounts/{account_id}/oauth2/custom-app-integrations",
  "description": "Get the list of custom OAuth app integrations for the specified Databricks account",
  "parameters": {
    "account_id": {
      "in": "path",
      "required": true,
      "type": "string"
    },
    "page_token": {
      "in": "query",
      "required": false,
      "type": "string"
    },
    "page_size": {
      "in": "query",
      "required": false,
      "description": "Default"
    },
    "100": {
      "in": "query",
      "required": false
    },
    "include_creator_username": {
      "in": "query",
      "required": false,
      "type": "boolean"
    }
  },
  "responses": {
    "200": {
      "apps": {
        "type": "Array of object",
        "description": "Array ["
      },
      "integration_id": {
        "type": "string",
        "description": "ID of this custom app"
      },
      "client_id": {
        "type": "string",
        "description": "The client id of the custom OAuth app"
      },
      "redirect_urls": {
        "description": "List of OAuth redirect urls"
      },
      "name": {
        "type": "string",
        "description": "The display name of the custom OAuth app"
      },
      "confidential": {
        "type": "boolean",
        "description": "Default"
      },
      "false": {
        "description": "This field indicates whether an OAuth client secret is required to authenticate this client."
      },
      "token_access_policy": {
        "description": "Token access policy"
      },
      "scopes": {
        "description": "Array of string"
      },
      "created_by": {
        "description": "int64"
      },
      "create_time": {
        "type": "string"
      },
      "creator_username": {
        "type": "string",
        "description": "]"
      },
      "next_page_token": {
        "type": "string"
      }
    },
    "401": {
      "error_code": "UNAUTHORIZED",
      "description": "The request does not have valid authentication credentials for the operation."
    },
    "500": {
      "error_code": "INTERNAL_SERVER_ERROR",
      "description": "Internal error."
    },
    "501": {
      "error_code": "NOT_IMPLEMENTED",
      "description": "Operation is not implemented or is not supported/enabled in this service."
    }
  }
}