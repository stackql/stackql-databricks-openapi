{
  "verb": "put",
  "path": "/api/2.0/preview/accounts/{account_id}/access-control/rule-sets",
  "description": "Replace the rules of a rule set.",
  "parameters": {
    "account_id": {
      "in": "path",
      "required": true,
      "type": "uuid",
      "description": "Databricks account ID of any type."
    }
  },
  "requestBody": {
    "properties": {
      "name": {
        "type": "string",
        "description": "Name of the rule set."
      },
      "rule_set": {
        "description": "object"
      },
      "grant_rules": {
        "type": "Array of object"
      },
      "etag": {
        "type": "string",
        "description": "not match the value that is stored in account access control service."
      }
    }
  },
  "responses": {
    "200": {
      "name": {
        "type": "string",
        "description": "Name of the rule set."
      },
      "grant_rules": {
        "type": "Array of object",
        "description": "Array ["
      },
      "principals": {
        "description": "Principals this grant rule applies to."
      },
      "role": {
        "type": "string",
        "description": "]"
      },
      "etag": {
        "type": "string",
        "description": "Identifies the version of the rule set returned."
      }
    },
    "400": {
      "error_code": "BAD_REQUEST",
      "description": "Request is invalid."
    },
    "401": {
      "error_code": "UNAUTHORIZED",
      "description": "The request does not have valid authentication credentials for the operation."
    },
    "500": {
      "error_code": "INTERNAL_SERVER_ERROR",
      "description": "Internal error."
    }
  }
}